token', response.data.token);\r\n\r\n        if (response.data.is_staff) {\r\n          // Si es administrador, redirige a la p谩gina de generaci贸n de ganador\r\n          this.$router.push('/generate-winner');\r\n        } else {\r\n          // Si no es administrador, muestra el mensaje de 茅xito\r\n          this.successMessage =\r\n            '隆Usted se ha inscrito al sorteo que consiste en una promoci贸n de un Hotel que regala una estad铆a de 2 noches con todo pagado para una pareja en el d铆a de San Valent铆n! 隆xito y buena suerte!';\r\n          this.errorMessage = ''; // Limpia cualquier mensaje de error\r\n        }\r\n\r\n        this.email = '';\r\n        this.password = '';\r\n      } catch (error) {\r\n        this.errorMessage = error.response?.data?.error || 'Credenciales inv谩lidas.';\r\n        this.successMessage = ''; // Limpia cualquier mensaje de 茅xito\r\n        console.error('Error en el inicio de sesi贸n:', error);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.login-container {\r\n  background: url('../assets/pareja.jpg') no-repeat center center fixed; /* Imagen de fondo */\r\n  background-size: cover; /* Ajusta la imagen al tama帽o de la pantalla */\r\n  height: 100vh; /* Altura completa de la pantalla */\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: column; /* Alinea el contenido verticalmente */\r\n}\r\n\r\nform {\r\n  background-color: rgba(255, 255, 255, 0.8); /* Fondo blanco transl煤cido */\r\n  padding: 20px;\r\n  border-radius: 10px;\r\n  box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\ninput {\r\n  width: 90%;\r\n  margin: 10px 0;\r\n  padding: 10px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 5px;\r\n}\r\n\r\nbutton {\r\n  padding: 10px 20px;\r\n  border: none;\r\n  border-radius: 5px;\r\n  background-color: #4caf50;\r\n  color: white;\r\n  cursor: pointer;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #45a049;\r\n}\r\n\r\n.success-container {\r\n  margin-top: 20px;\r\n  padding: 20px;\r\n  background-color: #e6ffe6;\r\n  border: 2px solid #4caf50;\r\n  border-radius: 5px;\r\n}\r\n\r\n.success-container p {\r\n  font-size: 1.2em;\r\n  font-weight: bold;\r\n  color: #4caf50;\r\n}\r\n\r\n.animation {\r\n  margin-top: 20px;\r\n}\r\n\r\n.sunglasses-emoji {\r\n  font-size: 3em;\r\n  animation: emoji-move 3s ease-in-out infinite;\r\n}\r\n\r\n@keyframes emoji-move {\r\n  0% {\r\n    transform: translateX(0);\r\n  }\r\n  50% {\r\n    transform: translateX(15px);\r\n  }\r\n  100% {\r\n    transform: translateX(0);\r\n  }\r\n}\r\n\r\n.error {\r\n  color: red;\r\n  margin-top: 10px;\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAiB;;EAD9BC,GAAA;EAQ+BD,KAAK,EAAC;;;EARrCC,GAAA;EAc2BD,KAAK,EAAC;;;uBAb/BE,mBAAA,CAcM,OAdNC,UAcM,G,0BAbJC,mBAAA,CAAuB,YAAnB,gBAAc,sBAClBA,mBAAA,CAIO;IAJAC,QAAM,EAAAC,MAAA,QAAAA,MAAA,MAHjBC,cAAA,KAAAC,IAAA,KAG2BC,QAAA,CAAAC,gBAAA,IAAAD,QAAA,CAAAC,gBAAA,IAAAF,IAAA,CAAgB;sBACrCJ,mBAAA,CAAgF;IAJtF,uBAAAE,MAAA,QAAAA,MAAA,MAAAK,MAAA,IAIsBC,KAAA,CAAAC,KAAK,GAAAF,MAAA;IAAEG,IAAI,EAAC,OAAO;IAACC,WAAW,EAAC,oBAAoB;IAACC,QAAQ,EAAR;iDAArDJ,KAAA,CAAAC,KAAK,E,mBACrBT,mBAAA,CAA8E;IALpF,uBAAAE,MAAA,QAAAA,MAAA,MAAAK,MAAA,IAKsBC,KAAA,CAAAK,QAAQ,GAAAN,MAAA;IAAEG,IAAI,EAAC,UAAU;IAACC,WAAW,EAAC,YAAY;IAACC,QAAQ,EAAR;iDAAnDJ,KAAA,CAAAK,QAAQ,E,6BACxBb,mBAAA,CAA6C;IAArCU,IAAI,EAAC;EAAQ,GAAC,gBAAc,qB,4BAE3BF,KAAA,CAAAM,cAAc,I,cAAzBhB,mBAAA,CAKM,OALNiB,UAKM,GAJJf,mBAAA,CAA2B,WAAAgB,gBAAA,CAArBR,KAAA,CAAAM,cAAc,kB,0BACpBd,mBAAA,CAEM;IAFDJ,KAAK,EAAC;EAAW,IACpBI,mBAAA,CAAsC;IAAjCJ,KAAK,EAAC;EAAkB,GAAC,IAAE,E,0BAXxCqB,mBAAA,gBAcaT,KAAA,CAAAU,YAAY,I,cAArBpB,mBAAA,CAA2D,KAA3DqB,UAA2D,EAAAH,gBAAA,CAAnBR,KAAA,CAAAU,YAAY,oBAdxDD,mBAAA,e","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}